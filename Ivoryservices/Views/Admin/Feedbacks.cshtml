@model Ivoryservices.Models.Feedbacks;

@{
    TempData["Admin"] = "ADMIN";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Feedbacks";
}

<h1>Feedbacks</h1>

@*<h4>Feedbacks</h4>*@

<div class="row">
    <div class="col-md-4"></div>
    <div class="col-md-4 card fin ">
        <form method="post"enctype="multipart/form-data" asp-controller="Admin" asp-action="Creates">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Description" class="control-label lbl"></label>
                <input asp-for="Description" class="form-control" style="font-size:1.5rem" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="F_Date" class="control-label lbl">Date</label>
                <input asp-for="F_Date" class="form-control" style="font-size:1.5rem" />
                <span asp-validation-for="F_Date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" style="font-size:1.5rem" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="FeedbackIndex" class="fin">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}