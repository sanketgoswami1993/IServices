@model Ivoryservices.Models.Feedbacks


@{
    TempData["Admin"] = "ADMIN";
      Layout = "~/Views/Shared/_Layout.cshtml";   
}
<h4>Feedbacks</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="FeedbackEdit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="F_Id" />
            <div class="form-group">
                <label asp-for="Description" class="control-label lbl"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="F_Date" class="control-label lbl"></label>
                <input asp-for="F_Date" class="form-control" />
                <span asp-validation-for="F_Date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="FeedbackIndex">Back to List</a>
</div>

